scalar Object

type Query{
    ping: String!
}

type Subscription {
    messaging(chat : MessageInput!): MessageOutput
}
input MessageInput{
    user: String!
    message: String!
    timestamp: Float!
    attachesType: String!
}

type MessageOutput{
    user: String!
    message: String!
    timestamp: Float!
    attachesType: String!
}
