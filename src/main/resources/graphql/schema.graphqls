    session: String
    user: String
    message: String
    timestamp: Float
    inputType: String
}

type MessageOutput{
    session: String
    user: String
    message: String
    timestamp: Float
    inputType: String
}

type AzureUser{

    displayName: String
    givenName: String
    surname: String
